.canvas
  height 380px
  margin 0px auto 0
  overflow hidden
  position relative
  top 0px
  text-indent -9999px
  width 380px
  z-index 10
.icon, .icon *
  position absolute
#cruz-azul
  .cruz-azul1
    background-color #2e3192
    height 380px
    width 380px
    border-radius(380px\/380px)
  .cruz-azul2
    background-color #ffffff
    border-radius(272px\/272px)
    height 272px
    left (( 380px - 272px ) / 2)
    top (( 380px - 272px ) / 2)
    width 272px
  .cruz-azul3
    background-color #ef3125
    height 152px
    left (( 380px - 152px ) / 2)
    top (( 380px - 152px ) / 2)
    width 152px
  .cruz-azul4
    background-color #ffffff
    border-radius(120px\/120px)
    height 120px
    left (( 380px - 120px ) / 2)
    top (( 380px - 120px ) / 2)
    width 120px
  .cruz-azul5
    background-color #2e3192
    height 100px
    left (( 380px - 30px ) / 2)
    top (( 380px - 100px ) / 2)
    width 30px
  .cruz-azul6
    background-color #2e3192
    height 30px
