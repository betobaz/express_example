    top (( 380px - 30px ) / 2)
    width 100px
  .cruz-azul7, .cruz-azul8
    color #2e3192
    font-size 26px
    font-weight bold
    height 18px
    left (( 380px - 152px ) / 2)
    text-indent 0
    width 152px
  .cruz-azul7
    top 81px 
  .cruz-azul8
    top 263px
  .cruz-azul9
    color #ffffff
    font-size 35px
    font-weight 900
    height 39px
    left (( 380px - 147px ) / 2)
    text-indent 0
    top 316px
    width 147px
    .m
      left 0
      top 0
      transform(rotate(20deg))
    .e
      left 32px
      top 6px
      transform(rotate(7deg))
    .x
      left 58px
      top 8px
    .i
      left 82px
      top 8px
    .c
      left 92px
      top 7px
      transform(rotate(-7deg))
    .o
      left 117px
      top 0px
      transform(rotate(-20deg))
  .estrella
    border-right 7px solid transparent
    border-bottom 21px solid #ffffff
    border-left 7px solid transparent
    display block
    height 0
    top 33px
    left 100px
    width 0
    &:before
      border-bottom 16px solid #ffffff
      border-left 23px solid transparent
      border-right 23px solid transparent
      position absolute
      top 13px
      left -22px
      content ''
      transform(rotate(35deg))
    &:after
      border-bottom 16px solid #ffffff
      border-left 23px solid transparent
      border-right 23px solid transparent
      position absolute
      top 13px
      left -23px
      content ''
      transform(rotate(-35deg))
